{
  "name": "meetnizer",
  "version": "1.0.0",
  "description": "A application to make your meeting productive",
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "lint": "standard --fix",
    "test": "standard && jest --coverage",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron": "electron .",
    "dev": "nf start -p 8070"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
