  "repository": "https://github.com/meetnizer/meetnizer",
  "keywords": [
    "Electron",
    "Meet",
    "Organizer"
  ],
  "author": "http://github.com/brunopenso, http://github.com/fabiorogeriosj",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^3.0.7",
    "electron": "^6.0.12",
    "eslint": "^5.16.0",
    "foreman": ">=3.0.1",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "react-scripts": "0.8.5",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "moment": "^2.24.0",
    "nedb": "^1.8.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-icons": "^3.9.0",
    "react-router-dom": "^5.1.2",
    "reactstrap": "^8.1.1"
  },
  "standard": {
    "globals": [
      "beforeAll",
